{
  "name": "electron-vite-project",
  "private": true,
  "version": "0.1.0",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build && electron-builder",
    "preview": "vite preview",
    "migration:run": "ts-node-esm electron/db/migration.ts run",
    "migration:revert": "ts-node-esm electron/db/migration.ts revert",
    "migration:generate": "ts-node-esm electron/db/generate-migration.ts",
    "rebuild": "electron-rebuild -f -w better-sqlite3",
    "postinstall": "electron-builder install-app-deps",
    "release": "release-it",
    "release:minor": "release-it minor --ci",
    "release:major": "release-it major --ci",
    "release:patch": "release-it patch --ci",
    "release:dry": "release-it --dry-run --only-version"
  },
  "dependencies": {
    "@vueuse/core": "^10.9.0",
    "ant-design-vue": "^4.2.6",
    "better-sqlite3": "^11.10.0",
    "electron-store": "^8.2.0",
    "pinia": "^2.1.7",
    "typeorm": "^0.3.24",
    "unocss": "^66.1.0",
    "unplugin-vue-components": "^0.26.0",
    "vue": "3.5.13",
    "vue-router": "^4.5.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@release-it/bumper": "^7.0.5",
    "@types/node": "^20.11.30",
    "@vitejs/plugin-vue": "^5.0.4",
    "electron": "^30.1.0",
    "electron-builder": "^24.13.3",
    "electron-rebuild": "^3.2.9",
    "lodash-es": "^4.17.21",
    "release-it": "^19.2.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-router": "^0.12.0",
    "vite": "^5.2.6",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vue-tsc": "^2.0.6"
  },
  "optionalDependencies": {
    "dmg-license": "^1.0.11"
  }
}
